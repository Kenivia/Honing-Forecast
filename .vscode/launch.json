{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome (Vite)",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:5173/Honing-Forecast/",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "preLaunchTask": "wasm-and-start-dev",
            "serverReadyAction": {
                "pattern": "Local:\\s+(https?://\\S+)",
                "uriFormat": "%s",
                "action": "openExternally"
            }
        },
