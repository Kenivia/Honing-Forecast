{
    "name": "honing-forecast",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "wasm-pack build crates/wasm --release --target web && npm run test && vite build",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist",
        "lint": "eslint .",
        "preview": "vite preview",
        "typecheck": "tsc --noEmit",
        "dev": "wasm-pack build crates/wasm --release --target web && vite",
        "test": "cargo test --profile release -- --show-output && vitest run",
        "test:ui": "vitest run"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/material": "^7.3.2",
        "@visx/event": "^3.10.0",
        "@visx/xychart": "^3.12.0",
        "react": "^18",
        "react-dom": "^18",
        "sortablejs": "^1.15.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^24.3.0",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@typescript-eslint/eslint-plugin": "^8.42.0",
        "@typescript-eslint/parser": "^8.42.0",
        "@vitejs/plugin-react": "^5.0.0",
        "concurrently": "^9.2.1",
        "eslint": "^9.34.0",
        "eslint-formatter-unix": "^8.40.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "gh-pages": "^6.3.0",
        "globals": "^16.3.0",
        "jsdom": "^27.0.0",
        "rollup-plugin-visualizer": "^6.0.3",
        "typescript": "^5.9.2",
        "vite": "^6.3.6",
        "vite-bundle-visualizer": "^1.2.1",
        "vite-plugin-wasm": "^3.5.0",
        "vitest": "^3.2.4"
    }
}
